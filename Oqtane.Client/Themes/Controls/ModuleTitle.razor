@using Oqtane.Themes
@namespace Oqtane.Themes.Controls
@inherits ContainerBase

@title

@code {
    string title = "";

    protected override Task OnParametersSetAsync()
    {
        title = ModuleState.Title;
        if (PageState.Control == "Settings")
        {
            title = PageState.Control;
        }
        return Task.CompletedTask;
    }
}
